<ul class='horizontal'>
  <li><%= link_to 'Home', root_path, class:'active' %></li>
  <% if current_user %>
    <li><%= link_to 'Logout', logout_path, method: :DELETE %></li>
    <li class = "label">Logged in as <%= current_user.name %></li>
    <li class='rightli' style="float:right"><%= link_to "Profile", dashboard_path %></li>
  <% else %>
    <li><%= link_to 'Sign Up', new_user_path %></li>
    <li><%= link_to "Login", login_path %></li>
  <% end %>
</ul>